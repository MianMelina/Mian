Private Sub uebernehmen_Click()
    'Textinhalt aus der Userform in die Zellen schreiben
    Worksheets("Dispotool").Range("C30").Value = dispoerstellen.KfzArt1.Value
    Worksheets("Dispotool").Range("C31").Value = dispoerstellen.KfzArt2.Value
    Worksheets("Dispotool").Range("C32").Value = dispoerstellen.KfzArt3.Value
    Worksheets("Dispotool").Range("C33").Value = dispoerstellen.KfzArt4.Value
    Worksheets("Dispotool").Range("C34").Value = dispoerstellen.KfzArt5.Value
    Worksheets("Dispotool").Range("C35").Value = dispoerstellen.KfzArt6.Value
    Worksheets("Dispotool").Range("P30").Value = dispoerstellen.Kennzeichen1.Value
    Worksheets("Dispotool").Range("P31").Value = dispoerstellen.Kennzeichen2.Value
    Worksheets("Dispotool").Range("P32").Value = dispoerstellen.Kennzeichen3.Value
    Worksheets("Dispotool").Range("P33").Value = dispoerstellen.Kennzeichen4.Value
    Worksheets("Dispotool").Range("P34").Value = dispoerstellen.Kennzeichen5.Value
    Worksheets("Dispotool").Range("P35").Value = dispoerstellen.Kennzeichen6.Value
    Worksheets("Dispotool").Range("G23").Value = dispoerstellen.Datum1.Value
    Worksheets("Dispotool").Range("R23").Value = dispoerstellen.Datum2.Value
    Worksheets("Dispotool").Range("L23").Value = dispoerstellen.Uhrzeit1.Value
    Worksheets("Dispotool").Range("W23").Value = dispoerstellen.Uhrzeit2.Value
    Worksheets("Dispotool").Range("Dispo").Value = dispoerstellen.lfdnr.Value
End Sub
